{
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"rounded-full": {
		"style": {
			"borderTopLeftRadius": 9999,
			"borderTopRightRadius": 9999,
			"borderBottomRightRadius": 9999,
			"borderBottomLeftRadius": 9999
		}
	},
	"rounded": {
		"style": {
			"borderTopLeftRadius": 4,
			"borderTopRightRadius": 4,
			"borderBottomRightRadius": 4,
			"borderBottomLeftRadius": 4
		}
	},
	"bg-blue-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(191 219 254 / var(--tw-bg-opacity))"
		}
	},
	"bg-blue-400": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(96 165 250 / var(--tw-bg-opacity))"
		}
	},
	"bg-red-300": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(252 165 165 / var(--tw-bg-opacity))"
		}
	},
	"p-2": {
		"style": {
			"paddingTop": 8,
			"paddingRight": 8,
			"paddingBottom": 8,
			"paddingLeft": 8
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-1": {
		"style": {
			"paddingTop": 4,
			"paddingBottom": 4
